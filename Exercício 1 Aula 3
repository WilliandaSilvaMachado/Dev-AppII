void main() {
 
  var nome = "Willian";
  String sobrenome = "da Silva Machado";
  
  String endereco = 'Rua Pesqueiro № 103';
  int idade = 17;
  
  print("Nome: " + nome+ " " + sobrenome);
  print("Endereço: $endereco");
  print("Idade:  $idade");
 }
