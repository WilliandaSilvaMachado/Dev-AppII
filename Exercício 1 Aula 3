void main() {
  var area = "Área total do trapézio é:";
  double base_maior = 30;
  double base_menor = 10;
  double altura = 70;
  //cálculo das bases
  double total_trapezio = base_maior + base_menor * altura / 2;

  print(area + " " + "$total_trapezio");
}

-----------------------------------------------------------------------
void main() {
 
  var area = "Área total do quadrado é: ";
  double base = 50;
  double altura = 100;
  
  double area_quadrado = base*altura;
  
  print ("-   ▉   Quadrado   ▉   -");
  print ("Base = " + "$base");
  print ("Altura = " + "$altura");
  print ("Calculo: Base x Altura");
  print (area + "" + "$area_quadrado");
 
 
 }
