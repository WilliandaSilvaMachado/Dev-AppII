void main() {
  
  List nomes = ["Willian", "Rita", "Vitória", "Jonatas", "Marcelo"];

  print("Todos os nomes: " + "$nomes");
  
 if(nomes.contains(nomes)) {
   print("É a mesma pessoa");
 } else {
    print("Estes nomes não conferem");
 }

  var outronome = "Willian";
  
  print("Nomes parecidos: " + "$outronome");

 if(nomes.contains(outronome)) {
   print("É a mesma pessoa?");
 } else {
    print("Os nomes não conferem");
 }

}
